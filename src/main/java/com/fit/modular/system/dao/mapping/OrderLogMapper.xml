<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fit.modular.system.dao.OrderLogMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.fit.modular.system.model.OrderLog">
        <id column="id" property="id" />
        <result column="order_sn" property="orderSn" />
        <result column="order_sub_sn" property="orderSubSn" />
        <result column="message" property="message" />
        <result column="status" property="status" />
        <result column="create_user_id" property="createUserId" />
        <result column="create_user_name" property="createUserName" />
        <result column="create_time" property="createTime" />
        <result column="update_user_id" property="updateUserId" />
        <result column="update_user_name" property="updateUserName" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, order_sn AS orderSn, order_sub_sn AS orderSubSn, message, status, create_user_id AS createUserId, create_user_name AS createUserName, create_time AS createTime, update_user_id AS updateUserId, update_user_name AS updateUserName, update_time AS updateTime
    </sql>

</mapper>
